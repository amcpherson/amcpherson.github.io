	Keywords = {Journal Article},
	Mesh = {Animals; Cell Line, Tumor; Chromosomes, Human, Pair 5; Comparative Genomic Hybridization; Gene Dosage; Genome-Wide Association Study; Humans; Leukemia, Myeloid, Acute; Mice; MicroRNAs},
	Month = {Jan},
