	Doi = {10.1371/journal.pmed.1002197},
	Journal = {PLoS Med},
	Journal-Full = {PLoS medicine},
	Keywords = {Journal Article},
	Month = {Dec},
	Number = {12},
	Pages = {e1002197},
	Pmc = {PMC5154502},
	Pmid = {27959929},
